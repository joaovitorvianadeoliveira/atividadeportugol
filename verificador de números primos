programa {
  funcao inicio() {
    inteiro x, valor = 1, divisores = 0
    escreva("escreva um valor")
    leia(x)

    se(x >0 ){
      enquanto(valor <= x){
        se(x % valor ==0){
          divisores++
      }
       valor++

        }
        se(divisores == 2)
          escreva("o numero ", x, "\té primo")
          senao{ escreva("\no numero ", x, "\t não é primo\n")

          }

          }
        }
        } 
